<table class="expense-table">
  <tr>
    <th>Number</th>
    <th>Date</th>
    <th>Description</th>
    <th>Amount</th>
  </tr>
  <% @expenses.each do |e| %>
  <tr>
    <td><%= e.id%></td>
    <td><%= e.date %></td>
    <td><%= e.description %></td>
    <td><%= e.amount %></td>
  </tr>
  <% end %>
</table>

