<table class="expense-table">
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th>Amount</th>
  </tr>
  <% @expenses.each do |e| %>
  <tr>
    <td><%= e.date %></td>
    <td><%= e.description %></td>
    <td><%= e.amount %></td>
  </tr>
  <% end %>
</table>

<a href="/expenses/new"><button type="button" class="btn btn-outline-light">Create Expense</button></a>



